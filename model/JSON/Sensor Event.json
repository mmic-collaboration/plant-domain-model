{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "title": "Sensor Event",
    "properties": {
        "sensor event id": {
            "type": "number"
        },
        "sensor id": {
            "type": "number"
        },
        "sensor event type id": {
            "type": "number"
        },
        "sensor event timestamp": {
            "type": "string"
        },
        "batch number": {
            "type": "string"
        },
        "bottle id": {
            "type": "number"
        },
        "sensor event extension": {
            "type": "object",
            "additionalProperties": false
        }
    },
    "additionalProperties": false,
    "required": [
        "sensor event id",
        "sensor id"
    ]
}